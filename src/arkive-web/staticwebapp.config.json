{
  "navigationFallback": {
    "rewrite": "/index.html",
    "exclude": ["/_next/*", "/images/*", "/*.ico", "/*.png", "/*.svg"]
  },
  "routes": [
    {
      "route": "/auth/consent-callback/",
      "headers": {
        "Cross-Origin-Opener-Policy": "same-origin-allow-popups"
      }
    }
  ]
}
